{
  "name": "achilles-heel",
  "version": "1.0.0",
  "description": "game",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "react-leaflet": "^2.7.0",
    "react-router": "^5.2.0",
    "travis-ci": "^2.2.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.22.0",
