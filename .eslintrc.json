{
    "root": true,
    "plugins": ["jest"],
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "quotes": ["error", "double"],
        "arrow-parens": "warn",
        "comma-dangle": 0,
        "react/jsx-filename-extension": [0],
        "no-nested-ternary": "off",
        "react/destructuring-assignment": "off",
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "class-methods-use-this": "warn",
        "react/prop-types": "warn"
        "camelcase": "warn"
    }
}
